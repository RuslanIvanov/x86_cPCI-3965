#SERVER SLAVE

# Configure Loopback
auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
    address 192.168.2.2
    netmask 255.255.255.0

auto eth1
iface eth1 inet static
    address 192.168.0.2
    netmask 255.255.255.0

auto eth4
iface eth4 inet static
    address 192.168.13.3
    netmask 255.255.255.0

auto eth4:0
iface eth4:0 inet static
    address 192.168.4.2
    netmask 255.255.255.0


auto eth3
iface eth3 inet static
    address 192.168.9.145
    netmask 255.255.255.0

auto eth2
iface eth2 inet static
    address 192.168.8.145
    netmask 255.255.255.0

auto eth5
iface eth5 inet static
    address 10.101.1.3
    netmask 255.0.0.0

auto eth5:0
iface eth5:0 inet static
    address 192.168.5.2
    netmask 255.255.255.0




 


